asnmpc_lib = asnmpc_lib
ampl_asnmpc_exe = ampl_asnmpc_exe
ALL_TARGETS = $(asnmpc_lib) $(ampl_asnmpc_exe)
ASLDIR = ../../../ThirdParty/ASL
DIRS = $(ASLDIR) ./src ./AmplSolver
TARGETDIRS = ./src ./AmplSolver



all: $(ALL_TARGETS)

install: all
	for d in $(TARGETDIRS); do (cd $$d; $(MAKE) install ); done

clean: 
	-for d in $(DIRS); do (cd $$d; $(MAKE) clean ); done

$(ampl_asnmpc_exe): $(asnmpc_lib) ASL
	$(MAKE) -C AmplSolver

$(asnmpc_lib): 
	$(MAKE) -C src

ASL: 
	$(MAKE) -C $(ASLDIR)
